<p id="notice"><%= notice %></p>

<h1>Listing Authots</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Password</th>
      <th>Password confirmation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @authots.each do |authot| %>
      <tr>
        <td><%= authot.username %></td>
        <td><%= authot.email %></td>
        <td><%= authot.password %></td>
        <td><%= authot.password_confirmation %></td>
        <td><%= link_to 'Show', authot %></td>
        <td><%= link_to 'Edit', edit_authot_path(authot) %></td>
        <td><%= link_to 'Destroy', authot, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Authot', new_authot_path %>
